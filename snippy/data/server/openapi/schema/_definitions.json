          "type": "array",
          "items": {
            "$ref": "#/definitions/requestData"
          }
        }
      },
      "required": [
        "data"
      ]
    },
    "responseData": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "snippet",
            "solution",
            "reference"
          ]
