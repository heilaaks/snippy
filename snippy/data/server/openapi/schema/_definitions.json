        "name": {
          "$ref": "#/definitions/attributeName"
        },
        "groups": {
          "$ref": "#/definitions/attributeGroups"
        },
        "tags": {
          "$ref": "#/definitions/attributeTags"
        },
        "links": {
          "$ref": "#/definitions/attributeLinks"
        },
